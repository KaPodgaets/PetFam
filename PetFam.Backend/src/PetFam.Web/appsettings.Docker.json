{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.Seq" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information"
      }
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "Seq",
        "Args": { "serverUrl": "http://seq:5341" }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "Database": "Server=postgres;Port=5432;Database=pet-fam;User Id=postgres;Password=postgres;"
  },
  "Minio": {
    "Endpoint": "minio:9000",
    "AccessKey": "minioadmin",
    "SecretKey": "minioadmin",
    "WithSsl": false
  },
  "JwtOptions": {
    "Issuer": "http://pet-fam.org/api",
    "Audience": "http://pet-fam.org",
    "SecurityKey": "afsdgsdfgnxc#esdgfjnhd,klsrthgsfxgn",
    "Expiration": 60
  },
  "RefreshSession": {
    "ExpireTime": 30
  },
  "SoftDeleteOptions": {
    "DaysBeforeRemove": 30
  },
  "Admin": {
    "UserName": "admin",
    "Password": "!Admin123",
    "Email": "admin@admin.com"
  }
}