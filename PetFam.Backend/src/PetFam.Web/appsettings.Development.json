{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.Seq" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information"
      }
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "Seq",
        "Args": { "serverUrl": "http://localhost:5341" }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "ConnectionStrings": {
      "Database": "Server=server;Port=port;Database=database;User Id=userId;Password=password;"
    }
  },
  "Minio": {
    "Endpoint": "localhost:9999",
    "AccessKey": "user",
    "SecretKey": "user",
    "WithSsl": false
  },
  "JwtOptions": {
    "Issuer": "http://pet-fam.org/api",
    "Audience": "http://pet-fam.org",
    "SecurityKey": "afsdgsdfgnxc#esdgfjnhd,klsrthgsfxgn",
    "Expiration": 60
  },
  "RefreshSession": {
    "ExpireTime": 30
  }
}